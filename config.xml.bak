<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phoneGap.UnileverScanApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>UnileverScanApp</name>
    <description>
       Unilever Scan App
    </description>
    <author email="youngyang@icatholic.net.cn" href="http://www.icatholiccloud.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-bluetooth-serial" spec="~0.4.7" />
    <engine name="android" spec="~6.2.3" />
	<platform name="android">
	   <preference name="Fullscreen" value="true" />
	</platform>
	<allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
</widget>
